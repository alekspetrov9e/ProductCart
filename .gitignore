# ======================================
# Symfony / PHP Project .gitignore
# ======================================

# Environment variables
.env
.env.local
.env.*.local
.env.test.local

# Symfony cache and logs
/var/
/vendor/

# Composer PHAR
/composer.phar

# PHPUnit
/phpunit.xml
/phpunit.xml.dist
/build/
/coverage/

# IDE config
.idea/
.vscode/

# OS files
.DS_Store
Thumbs.db

# Node modules (if using Encore, Webpack, etc.)
/node_modules/
/public/build/
/public/bundles/

# Yarn / npm logs
yarn-error.log
npm-debug.log

# Other logs or temp files
*.log
*.cache
*.tmp
*.temp
*.xml

# Uploaded files (if any)
public/uploads/

# Ignore local database (if SQLite is used)
var/data.db
var/data.db-journal

# Local override files
/config/secrets/prod/prod.decrypt.private.php

# Ignore coverage reports
coverage/

# Ignore profiler data
var/cache/dev/profiler/

